{
  "name": "byjg/soap-server",
  "type": "library",
  "description": "A lightweight and modern SOAP server implementation for PHP",
  "autoload": {
    "psr-4": {
      "ByJG\\SoapServer\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1 <8.5",
    "ext-soap": "*",
    "ext-dom": "*",
    "byjg/jinja-php": "^6.0",
    "byjg/webrequest": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.5",
    "vimeo/psalm": "^5.9|^6.13"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Joao Gilberto Magalhaes",
      "email": "joao@byjg.com.br"
    }
  ],
  "scripts": {
    "test": "phpunit",
    "psalm": "psalm"
  }
}
